
a: DF 1.0
b: DF -1.0
c: DF 2.0

ecall f3, x0, 6 ; input
ecall x5, x0, 5 ; precision input

fld f1, a(x0)
fld f2, b(x0)
fld f31, c(x0)

fadd.d f4, f3, f1 ;high
fadd.d f5, f0, f1	;low

loop: 	beq x5, x0, end
	fadd.d f6, f4, f5;sum
	fdiv.d f7, f6, f31 ;avg
	fmul.d f8, f7, f7 ; square of avg
	flt.d x1, f8, f3
	
end:	ebreak x0,x0,0